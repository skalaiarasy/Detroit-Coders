@model ProgramOrganizerViewModel

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    body {
        background-color: azure;
    }
</style>

<h2 class="text-info">Enroll Organizer</h2>
<div class="border backgroundwhite">
    <form enctype="multipart/form-data" asp-action="Create">
        @*<div class="row">*@
            <div class="col-md-4">

                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="Organizer.Name" class="control-label"></label>
                    <input asp-for="Organizer.Name" class="form-control" />
                    <span asp-validation-for="Organizer.Name" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Organizer.Address" class="control-label"></label>
                    <input asp-for="Organizer.Address" class="form-control" />
                    <span asp-validation-for="Organizer.Address" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Organizer.City" class="control-label"></label>
                    <input asp-for="Organizer.City" class="form-control" />
                    <span asp-validation-for="Organizer.City" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Organizer.State" class="control-label"></label>
                    <input asp-for="Organizer.State" class="form-control" />
                    <span asp-validation-for="Organizer.State" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Organizer.Zipcode" class="control-label"></label>
                    <input asp-for="Organizer.Zipcode" class="form-control" />
                    <span asp-validation-for="Organizer.Zipcode" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Organizer.Country" class="control-label"></label>
                    <input asp-for="Organizer.Country" class="form-control" />
                    <span asp-validation-for="Organizer.Country" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-primary" />
                </div>

            </div>
    </form>
</div>

@*<div>
        <a asp-action="Index">Back to List</a>
    </div>*@


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
